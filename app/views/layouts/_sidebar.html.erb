<div class="col-md-3">
  <div class="list-group">
    <div class="add-to-cart-button">
      <%= link_to admin_static_pages_path do %>
        <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
        <%= t "dashboard" %>
      <% end %>
    </div>
    <%= link_to admin_brands_path, class: "list-group-item" do %>
      <span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>
      <%= t "brand" %>
    <% end %>
    <%= link_to admin_products_path, class: "list-group-item" do %>
      <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
      <%= t "product" %>
    <% end %>
    <%= link_to admin_coupons_path, class: "list-group-item" do %>
      <span class="glyphicon glyphicon-user" aria-hidden="true"></span>
      <%= t "coupon" %>
    <% end %>
    <%= link_to admin_orders_path, class: "list-group-item" do %>
      <span class="glyphicon glyphicon-user" aria-hidden="true"></span>
      <%= t "manage_order" %>
    <% end %>
  </div>
</div>
